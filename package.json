{
  "name": "cloud-blog",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:remix": "remix dev --manual -c \"npm run start\"",
    "dev:docker": "docker compose -f docker/development/docker-compose.yml up -d",
    "dev:codegen": "graphql-codegen -c codegen/codegen.json -w",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --fix --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "dev:proxy": "prisma-accelerate-local postgresql://postgres:password@localhost:15432/postgres -p 8000 -t",
    "dev:css": "tcm app --ignore **/tailwindcss.css -w -a",
    "start": "wrangler --port 3000 pages dev ./public",
    "type-check": "tsc",
    "build": "remix build",
    "pages:deploy": "npm run build && wrangler pages deploy ./public",
    "prisma": "npm-run-all -p prisma:*",
    "prisma:migrate": "prisma format && next-exec prisma migrate dev",
    "prisma:generate": "prisma generate --no-engine",
    "graphql:schema": "tsx ./tools/exportSchema.ts"
  },
  "dependencies": {
    "@apollo/explorer": "^3.6.0",
    "@firebase/app": "^0.9.27",
    "@firebase/auth": "^1.6.0",
    "@monaco-editor/react": "^4.6.0",
    "@pothos/core": "^3.41.0",
    "@pothos/plugin-prisma": "^3.63.1",
    "@pothos/plugin-prisma-utils": "^0.14.0",
    "@prisma/adapter-pg": "^5.10.0-dev.10",
    "@prisma/client": "^5.9.1",
    "@react-libraries/next-exchange-ssr": "^1.0.4",
    "@remix-run/cloudflare": "^2.6.0",
    "@remix-run/cloudflare-pages": "^2.6.0",
    "@remix-run/css-bundle": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "@urql/exchange-retry": "^1.2.1",
    "cookie": "^0.6.0",
    "daisyui": "^4.6.2",
    "firebase-storage": "^1.0.0",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.1.1",
    "isbot": "^4.4.0",
    "jose": "^5.2.2",
    "monaco-editor": "^0.46.0",
    "pothos-prisma-generator": "^1.0.2",
    "pothos-schema-exporter": "^1.0.0",
    "pure-uuid": "^1.8.1",
    "react": "^18.2.0",
    "react-daisyui": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-icons": "^5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.5.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "urql": "^4.0.6",
    "use-deep-compare-effect": "^1.8.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.4",
    "@graphql-codegen/typescript-operations": "^4.1.2",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@node-libraries/semaphore": "^1.0.3",
    "@parcel/watcher": "^2.4.0",
    "@remix-run/dev": "^2.6.0",
    "@storybook/react": "^7.6.14",
    "@types/mdast": "^4.0.3",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql-tag": "^2.12.6",
    "next-exec": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.9.1",
    "prisma-accelerate-local": "^1.0.1",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.3.3",
    "wrangler": "3.28.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "description": "Cloudflare Pages & Remix Blog system.",
  "version": "1.0.0",
  "main": "remix.config.js",
  "keywords": [],
  "author": "SoraKumo <info@croud.jp>",
  "license": "ISC",
  "repository": "https://github.com/SoraKumo001/cloud-blog"
}
