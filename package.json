{
  "name": "cloud-blog",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:react-router": "react-router dev --port 3000",
    "dev:docker": "docker compose -f docker/development/docker-compose.yml up -d",
    "dev:codegen": "graphql-codegen -c codegen/codegen.ts -w",
    "lint": "eslint app",
    "lint:fix": "eslint --fix app",
    "start": "pnpm run build && wrangler dev",
    "typecheck": "tsc --noEmit --pretty",
    "typegen": "wrangler types",
    "build": "react-router build",
    "deploy": "pnpm run build && wrangler deploy",
    "dry-run": "wrangler deploy --dry-run",
    "deploy-secret": "wrangler secret bulk .env.production",
    "prisma-": "npm-run-all -p prisma:*",
    "prisma:migrate": "prisma format && prisma migrate dev",
    "prisma:generate": "prisma generate",
    "graphql:schema": "tsx --tsconfig tsconfig.cloudflare.json ./tools/exportSchema.ts",
    "log": "wrangler tail cloud-blog"
  },
  "dependencies": {
    "@firebase/app": "^0.14.3",
    "@firebase/auth": "^1.11.0",
    "@monaco-editor/react": "^4.7.0",
    "@node-libraries/semaphore": "^1.0.3",
    "@pothos/core": "^4.9.1",
    "@pothos/plugin-prisma": "^4.11.0",
    "@pothos/plugin-prisma-utils": "^1.3.3",
    "@prisma/adapter-pg": "^6.16.2",
    "@prisma/client": "^6.16.2",
    "@react-libraries/next-exchange-ssr": "^1.0.9",
    "@react-router/dev": "^7.9.3",
    "@react-router/fs-routes": "^7.9.3",
    "@urql/exchange-retry": "^2.0.0",
    "apollo-explorer": "1.1.3",
    "blurhash": "^2.0.5",
    "cloudflare-fonts": "0.0.2",
    "cookie": "^1.0.2",
    "firebase-storage": "^1.1.4",
    "graphql": "^16.11.0",
    "graphql-yoga": "^5.16.0",
    "hono": "^4.9.9",
    "jose": "^6.1.0",
    "monaco-editor": "^0.53.0",
    "pothos-prisma-generator": "^1.3.10",
    "prism-react-renderer": "^2.4.1",
    "pure-uuid": "^1.8.1",
    "react": "19.1.1",
    "react-daisyui": "^5.0.5",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.9.3",
    "react-router-dom": "^7.9.3",
    "rehype-raw": "7.0.0",
    "rehype-react": "8.0.0",
    "remark-breaks": "4.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.2",
    "unified": "11.0.5",
    "unist-util-visit": "5.0.0",
    "urql": "^5.0.1",
    "vfile": "6.0.3",
    "wasm-image-optimization": "1.3.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.13.7",
    "@cloudflare/workers-types": "^4.20250927.0",
    "@eslint/js": "9.36.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-codegen/typescript-urql": "^4.0.1",
    "@hono/vite-dev-server": "^0.21.1",
