{
  "name": "cloud-blog",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:remix": "remix vite:dev --port 3000",
    "dev:docker": "docker compose -f docker/development/docker-compose.yml up -d",
    "dev:codegen": "graphql-codegen -c codegen/codegen.json -w",
    "dev:proxy": "prisma-accelerate-local postgresql://postgres:password@localhost:25432/postgres -p 8000 -t",
    "dev:css": "tcm app --ignore **/tailwindcss.css -w",
    "build": "remix vite:build",
    "deploy": "remix vite:build && wrangler pages deploy ./build/client",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --fix --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "pages:deploy": "npm run build && wrangler pages deploy ./build/client",
    "pages:deploy:preview": "npm run build && wrangler pages deploy --branch development ./build/client",
    "prisma": "npm-run-all -p prisma:*",
    "prisma:migrate": "prisma format && next-exec prisma migrate dev",
    "prisma:generate": "prisma generate --no-engine",
    "graphql:schema": "tsx ./tools/exportSchema.ts",
    "log": "wrangler pages deployment tail --project cloud-blog"
  },
  "dependencies": {
    "@apollo/explorer": "^3.6.0",
    "@firebase/app": "^0.10.0",
    "@firebase/auth": "^1.7.0",
    "@monaco-editor/react": "^4.6.0",
    "@node-libraries/wasm-avif-encoder": "^1.0.1",
    "@node-libraries/wasm-webp-encoder": "^1.1.1",
    "@pothos/core": "^3.41.0",
    "@pothos/plugin-prisma": "^3.65.0",
    "@pothos/plugin-prisma-utils": "^0.14.0",
    "@prisma/client": "^5.11.0",
    "@react-libraries/next-exchange-ssr": "^1.0.6",
    "@remix-run/cloudflare": "^2.8.1",
    "@remix-run/cloudflare-pages": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@urql/exchange-retry": "^1.2.1",
    "blurhash": "^2.0.5",
    "cookie": "^0.6.0",
    "daisyui": "^4.9.0",
    "firebase-storage": "^1.1.4",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.3.0",
    "isbot": "^5.1.3",
    "jose": "^5.2.3",
    "miniflare": "^3.20240320.1",
    "monaco-editor": "^0.47.0",
    "pothos-prisma-generator": "^1.0.2",
    "pothos-schema-exporter": "^1.0.0",
    "prism-react-renderer": "^2.3.1",
    "pure-uuid": "^1.8.1",
    "react": "^18.2.0",
    "react-daisyui": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.2",
    "react-icons": "^5.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "urql": "^4.0.6",
    "use-deep-compare-effect": "^1.8.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240329.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@parcel/watcher": "^2.4.1",
    "@remix-run/dev": "^2.8.1",
    "@storybook/react": "^8.0.5",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql-tag": "^2.12.6",
    "next-exec": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.11.0",
    "prisma-accelerate-local": "^1.1.2",
    "tsx": "^4.7.1",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.7",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^3.41.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
